		<Platform
			Name="Win32"
		/>
	</Platforms>
	<ToolFiles>
	</ToolFiles>
	<Configurations>
		<Configuration
			Name="Release|Win32"
			OutputDirectory="..\..\build\$(ConfigurationName)\perl5"
			IntermediateDirectory="..\..\build\$(ConfigurationName)\obj\$(ProjectName)_perl"
			ConfigurationType="2"
			CharacterSet="1"
			WholeProgramOptimization="1"
