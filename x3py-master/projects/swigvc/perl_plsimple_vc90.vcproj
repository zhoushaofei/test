			Filter="i;py;pyw;java;cs;pl;rb"
			UniqueIdentifier="{B66BBF33-21A8-451F-A173-F4C554C67AFE}"
			>
			<File
				RelativePath="..\..\source\example\plsimple\plsimple.i"
				>
				<FileConfiguration
					Name="Release|Win32"
					>
					<Tool
						Name="VCCustomBuildTool"
						Description="Swig generating..."
						CommandLine="if not exist $(TargetDir) mkdir $(TargetDir)&#x0D;&#x0A;$(SWIG_BIN)swig -c++ -perl5 -I&quot;$(ProjectDir)..\..\source\public&quot; -I&quot;$(ProjectDir)..\..\interface\core&quot; -I&quot;$(ProjectDir)..\..\interface\example&quot; -o &quot;$(InputDir)$(InputName)_perl_wrap.cxx&quot; -outdir $(TargetDir) $(InputPath)&#x0D;&#x0A;"
						Outputs="$(InputDir)$(InputName)_perl_wrap.cxx"
					/>
				</FileConfiguration>
			</File>
			<File
				RelativePath="..\..\source\example\plsimple\plsimple_perl_wrap.cxx"
				>
				<FileConfiguration
					Name="Release|Win32"
					>
